{
  "name": "WU",
  "version": "1.0.0",
  "description": "Hardware Dashboard API - Shipping Label Creator (WU)",
  "bin": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "build:exe": "npm run build && pkg . --targets node18-win-x64 --output WU.exe"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "inquirer": "^8.2.6",
    "ora": "^5.4.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.6",
    "@types/node": "^20.11.30",
    "@types/uuid": "^9.0.1",
    "pkg": "^5.8.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": []
  }
}
